  "keywords": [
    "aliasedemail"
  ],
  "author": "mikikiv",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
